   "source": [
    "## El metodo items() nos devuelve Clave y valor automaticamente "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "id": "1121126a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Emanuel 24\n",
      "Adriel 26\n",
      "Marianela 22\n",
      "Aldana 16\n",
      "Silvana 66\n"
     ]
    }
   ],
   "source": [
    "for c,v in edades.items():\n",
    "    print(c,v)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "id": "8f1e7187",
   "metadata": {},
   "outputs": [
    {
